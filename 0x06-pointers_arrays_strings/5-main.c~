#include "main.h"
#include <stdio.h>

/**
 * main - check the code
 *
 * Return: Always 0.
 *
 */
char *string_toupper(char *str);

int main(void)
{
char str[] = "Hello, World! This is a Test String.";
    
printf("Original: %s\n", str);
    
char *ptr = string_toupper(str);
    
printf("Uppercase: %s\n", ptr);
    
return (0);
}
